C.... MAXAT : MAXIMUM NUMBER OF ATOMS SUPPORTED
C.... N_ORB : MAXIMUM NUMBER OF ORBITALS

      PARAMETER(MAXAT=20, N_ORB=60, NTBIEL=((N_ORB*(N_ORB+1))/2)**2)

C.... HIGH QUANTUM NUMBERS

      PARAMETER(L1MAX=3, L2MAX=3, L3MAX=3, L4MAX=3,
     $                                   L1234M=L1MAX+L2MAX+L3MAX+L4MAX)

      PARAMETER(L1M1=L1MAX+1, L2M1=L2MAX+1, L3M1=L3MAX+1, L4M1=L4MAX+1)

C.... THE HIGHEST ORDER OF THE COMPUTED TERMS.

      PARAMETER (LMDMAX=16, LDEV=20+L1234M)

C.... INTEGRATION PARAMETERS FOR THE BESSEL COEFFICIENTS.
C.... L001, L002, L003 : ORDERS OF THE G-LEGENDRE QUADRATURES
C.... USED TO COMPUTE THE INTEGRAL REPRESENTATION OVER [0, 1]

      PARAMETER(L001=14, L002=24, L003=20)

C.... L004 AND L005 ORDERS OF THE G-LEGENDRE QUADRATURES USED
C... OVER [0, 1] WHEN ZETA IS LOWER THAN 1.
C.... LAG01 IS THE ORDER OF THE G-LAGUERRE QUADRATURE FOR ZETA >=1 OVER
C.... $[1, +\INFTY)$

      PARAMETER (L004=08, L005=14, L006=24)

      PARAMETER (L1_3 = L001+L002+L003, L4_6 = L004+L005+L006,
     $                                                LTOT6 = L1_3+L4_6)

C.... ******************************************************************
C.... MAXIMUM NUMBER OF ATOMIC ORBITALS ON A GIVEN ATOM

      PARAMETER (NORBM=46)

C.... INTEGRATION PARAMETERS FOR THE OVERLAP AND THE TWO-CENTER KINETIC INTEGRALS

      PARAMETER (LEG00=12, LAG00=08, LEA00=LEG00+LAG00)

C.... INTEGRATION PARAMETERS FOR THE THREE-CENTER ONE-ELECTRON INTEGRALS

c      PARAMETER (LEG01=16, LEG02=16, LAG01=10, LEA01=LEG01+LAG01+LEG02)
      PARAMETER (LEG01=16, LEG02=16, LAG01=8, LEA01=LEG01+LAG01+LEG02)

C.... INTEGRATION PARAMETERS FOR THE HYBRID INTEGRALS

      PARAMETER (LEG03=16, LAG02=16, LEA02=LEG03+LAG02)

C.... INTEGRATION PARAMETERS FOR THE TWO-CENTER COULOMB INTEGRALS

      PARAMETER (LEG04=16, LAG03=16, LEA03=LEG04+LAG03)

C.... INTEGRATION PARAMETERS FOR THE THREE-CENTER TWO-ELECTRON COULOMB INTEGRALS

c      PARAMETER (LEG05=16, LEG06=16, LAG04=10, LEA04=LEG05+LEG06+LAG04)
      PARAMETER (LEG05=16, LEG06=16, LAG04=16, LEA04=LEG05+LEG06+LAG04)

C.... INTEGRATION PARAMETERS FOR THE TWO-CENTER EXCHANGE INTEGRALS

      PARAMETER (LEG07=16, LAG05=24, LEA05=LEG07+LAG05)
      PARAMETER (LEG08=16, LEG09=16, LAG06=16, LEA06=LEG08+LEG09+LAG06)

C.... INTEGRATION PARAMETERS FOR THE THREE-CENTER EXCHANGE INTEGRALS

      PARAMETER (LEG10=12, LEG11=12, LAG07=16, LEA07=LEG10+LEG11+LAG07)
      PARAMETER (LEG12=12, LEG13=12, LEG14=12, LAG08=12,
     $                                   LEA08=LEG12+LEG13+LEG14+LAG08)
cc      PARAMETER (LEG10=16, LEG11=16, LAG07=16, LEA07=LEG10+LEG11+LAG07)
cc      PARAMETER (LEG12=16, LEG13=16, LEG14=16, LAG08=12,
cc     $                                   LEA08=LEG12+LEG13+LEG14+LAG08)

c      PARAMETER (LEG10=16, LAG07=12, LEA07=LEG10+LAG07)
c      PARAMETER (LEG12=12, LEG13=12, LAG08=12, LEA08=LEG12+LEG13+LAG08)

C.... INTEGRATION PARAMETERS FOR THE ONE-CENTER TWO-ELECTRON INTEGRALS

      PARAMETER (LAG09=20, LEA09=LAG09)

C.... INTEGRATION PARAMETERS FOR THE TWO-CENTER ONE-ELECTRON INTEGRALS (NOT OVERLAP)

      PARAMETER (LEG15=20, LAG10=12, LEA10=LEG15+LAG10)

C.... INTEGRATION PARAMETERS FOR THE FOUR-CENTER INTEGRALS
C.... [R2=0, R2=AC](LEG16), [R2=AC, R2=AD](LEG17), [R2=AD, +\INFTY[(LAG11)
C.... [R1=0, R1=R2](LEG18), [R1=R2, R1=AB](LEG19), [R1=AB, +\INFTY[(LAG12)

C.... LAG11 ET LAG12 : 10 TOUS LES AUTRES SONT A 12

      PARAMETER (LEG16=12, LEG17=12 , LAG11=08 ,LEA11=LEG16+LEG17+LAG11)
      PARAMETER (LEG18=12 , LEG19=12, LAG12=08, LEA12=LEG18+LEG19+LAG12)

C.... NOW, LET US DETERMINE THE MAXIMUM VALUE FOR THE HEREX ARRAY

      PARAMETER (LMHERM=MAX(LEA00, LEA01, LEA02, LEA03,
     $   LEA04, LEA05, LEA06, LEA07, LEA08, LEA09, LEA10, LEA11, LEA12))
